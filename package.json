{
  "name": "@vaco/core",
  "version": "0.0.0-development",
  "description": "Library that extends the constraint validation api.",
  "keywords": [
    "validation"
  ],
  "homepage": "https://github.com/owner/project#readme",
  "bugs": "https://github.com/owner/project/issues",
  "license": "MIT",
  "author": "JÃ¼rgen Steitz <juergen@steitz.work>",
  "files": [
    "dist/*"
  ],
  "type": "module",
  "browser": "lib/index.umd.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "doc": "docs",
    "test": "tests"
  },
  "repository": "github:user/repo",
  "scripts": {
    "build": "rollup -c --environment PRODUCTION",
    "test": "rollup -c --environment TEST && node -r dotenv/config tools/test.local.js",
    "test:ci": "rollup -c --environment TEST && node -r dotenv/config tools/test.browserstack.js",
    "coverage:test": "rollup -c --environment TEST,COVERAGE && node -r dotenv/config tools/test.local.js",
    "coverage": "npm run coverage:test && nyc report --reporter=lcov --reporter=text",
    "lint": "tsc --noEmit && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/node": "^14.14.7",
    "@types/selenium-webdriver": "^4.0.10",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "browserstack-local": "^1.4.8",
    "chalk": "^4.1.0",
    "chromedriver": "^86.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "nyc": "^15.1.0",
    "rollup": "^2.33.1",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-istanbul": "git+https://github.com/billowz/rollup-plugin-istanbul.git#rc",
    "rollup-plugin-terser": "^7.0.2",
    "selenium-webdriver": "^4.0.0-alpha.6",
    "semantic-release": "^17.2.2",
    "tap-mocha-reporter": "^5.0.1",
    "typescript": "^4.0.5",
    "zora": "^4.0.1"
  },
  "peerDependencies": {
    "@vaco/reporters": "*",
    "@vaco/constraints": "*",
    "@vaco/i18n": "*"
  },
  "hooks": {
    "pre-push": "npm run lint"
  }
}
